#include<bits/stdc++.h>
using namespace std;
int findfrequency(vector<int>&v, int x){
    int count =0;
    
    for(int i =0;i<v.size();i++){
        if(v[i]==x){
        count++;
        }
    }
    return count;
}
int main (){
    vector <int> v={1,3,4,5,5,5,5,7,8};
    int x;
    cin>>x;
    cout<<"the frequency of "<< x <<" is "<< findfrequency(v,x)<<endl;
    return 0 ;
}
